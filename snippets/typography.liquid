{% comment %} <style>
  @font-face {
    font-family: 'MyCustomFont';
    src: url('{{ ' my-custom-font.woff2' | asset_url }}') format('woff2'),
         url('{{ ' my-custom-font.woff' | asset_url }}') format('woff');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
  }

  body {
    font-family: 'MyCustomFont', sans-serif;
  }
</style>

<link rel="preload" href="{{ 'my-custom-font.woff2' | asset_url }}" as="font" type="font/woff2" crossorigin="anonymous"> {% endcomment %}